{
  "name": "flatlist-react",
  "version": "1.5.3",
  "description": "A helpful utility component to handle lists in react like a champ",
  "main": "./lib/index.js",
  "scripts": {
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "eslint src/**/*.ts{,x}",
    "lint:fix": "eslint --fix src/**/*.ts{,x}",
    "tsc": "tsc",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run format && npm run lint && npm run tsc",
    "build:local": "npm run format && npm run lint && npm run tsc",
    "local": "nodemon --watch ./src -e ts,tsx --exec npm run build:local",
    "test": "jest --silent",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beforesemicolon/flatlist-react"
  },
  "keywords": [
    "react",
    "utility",
    "flatlist"
  ],
  "author": "Elson T. Correia",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.8.1",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/core-js": "^2.5.5",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/warning": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
